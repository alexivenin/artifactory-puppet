<Server port="8015" shutdown="SHUTDOWN">

    <Service name="Catalina">
        <Connector port="<%= @artifactory_tomcat_port %>" protocol="HTTP/1.1"
        maxThreads="<%= @artifactory_tomcat_maxthreads %>" minSpareThreads="<%= @artifactory_tomcat_minsparethreads %>"
        enableLookups="<%= @artifactory_tomcat_enablelookups %>" disableUploadTimeout="<%= @artifactory_tomcat_disableuploadtimeout %>"
        backlog="<%= @artifactory_tomcat_backlog %>"/>

        <!-- This is the optional AJP connector -->
        <Connector port="8019" protocol="AJP/1.3"/>

        <Engine name="Catalina" defaultHost="localhost">
            <Host name="localhost" appBase="webapps"/>
        </Engine>

    </Service>
</Server>

